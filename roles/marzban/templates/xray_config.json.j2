            "xver": 2
          },
          {
            "dest": "/run/nginx-h1.socket",
            "xver": 2
          }
        ]
      },
      "streamSettings": {
        "network": "tcp",
        "security": "tls",
        "tlsSettings": {
          "serverName": "{{ marzban_subdomain }}.{{ marzban_domain }}",
          "certificates": [
            {
              "ocspStapling": 3600,
              "certificateFile": "{{ marzban_container_data_dir }}/certs/fullchain.pem",
              "keyFile": "{{ marzban_container_data_dir }}/certs/key.pem"
            }
          ],
          "minVersion": "1.2",
          "cipherSuites": "TLS_ECDHE_ECDSA_WITH_CHACHA20_POLY1305_SHA256:TLS_ECDHE_RSA_WITH_CHACHA20_POLY1305_SHA256:TLS_ECDHE_ECDSA_WITH_AES_256_GCM_SHA384:TLS_ECDHE_RSA_WITH_AES_256_GCM_SHA384:TLS_ECDHE_ECDSA_WITH_AES_128_GCM_SHA256:TLS_ECDHE_RSA_WITH_AES_128_GCM_SHA256",
          "alpn": ["h2", "http/1.1"]
        }
      },
      "sniffing": {
        "enabled": true,
        "destOverride": ["http", "tls"]
      }
    },
    {
      "tag": "TROJAN_FALLBACK",
      "port": 334,
      "protocol": "trojan",
      "settings": {
        "clients": [],
        "decryption": "none",
        "fallbacks": [
          {
            "alpn": "h2",
            "dest": "/run/nginx-h2c.socket",
            "xver": 2
