        ],
        "decryption": "none"
      },
      "streamSettings": {
        "network": "ws",
        "security": "none",
        "wsSettings": {
          "acceptProxyProtocol": true, //Enable PROXY protocol reception, receive the real source IP and port
          "path": "/vlws" // If you change path, it should also be changed in fallback rules
